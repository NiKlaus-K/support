/**app.wxss**/
@import '/style/weui.wxss';
@import '/images/icons/iconfont.wxss';
@import '/style/dist/step/index.wxss';

/* 设置主内容区域 */

.container {
  min-height: 100vh;
  width: 100%;
  background: #f7f7f7;
  padding-bottom: 60rpx;
}

/* =================分割区域样式=====开始================== */
/* 分割块 */
.divBlank {
  background-color: #fafbfd;
  height: 0.3rem;
  width: 120%;
}
/* 分割线 */
.divLine {
    margin: 0;
    background: #e4e4e4;
    width: 100%;
    height: 3rpx;
}
/* =================分割区域样式=====结束================== */

